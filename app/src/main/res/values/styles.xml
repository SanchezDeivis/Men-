    <style name="TextInputLAyoutAppearance" parent="Widget.MaterialComponents.TextInputLayout.FilledBox.Dense">
        <!-- parent="TextAppearance.AppCompat"-->
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/colorGris</item>
        <item name="android:textSize">15sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/colorRojo</item>
        <item name="colorControlNormal">@color/colorGris</item>
        <item name="colorControlActivated">@color/colorGris</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:password">false</item>
